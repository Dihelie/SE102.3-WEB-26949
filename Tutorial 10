      <script>
            var num1=parseInt(prompt("Enter Number 01:"));
            var num2=parseInt(prompt("Enter Number 02:"));
            document.write("Addition:",num1+num2);
            document.write("<br>");
            document.write("Subtraction:",num1-num2);
            document.write("<br>");
            document.write("Division:",num1/num2);
            document.write("<br>");
            document.write("Multiplication:",num1*num2);
      </script>
      
</body>
